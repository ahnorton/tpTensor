          RowBox[{"task", "[", 
           RowBox[{"4", ",", "nb"}], "]"}], "]"}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Quiet", "[", 
         RowBox[{"RemoveScheduledTask", "[", 
          RowBox[{"task", "[", 
           RowBox[{"5", ",", "nb"}], "]"}], "]"}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"DeleteFile", "[", 
