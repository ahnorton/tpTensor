  RowBox[{"StopBackup", "[", "]"}], ":=", 
  RowBox[{"StopBackup", "[", 
   RowBox[{"EvaluationNotebook", "[", "]"}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"StopBackup", "[", "nb_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"msg", ",", "dir"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"dir", "=", 
      RowBox[{"StringReplace", "[", 
       RowBox[{
        RowBox[{"NotebookFileName", "[", "nb", "]"}], ",", 
        RowBox[{"\"\<.nb\>\"", "\[Rule]", 
         RowBox[{"\"\<_bak\>\"", "<>", "$PathnameSeparator"}]}]}], "]"}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"!", 
        RowBox[{"FileExistsQ", "[", "dir", "]"}]}], ",", 
       RowBox[{
        RowBox[{
        "msg", "=", 
         "\"\<The backup directory could not be found. Perhaps backup failed \
to start?\>\""}], ";", " ", 
        RowBox[{"Return", "[", "msg", "]"}]}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"BackupRunningQ", "[", "nb", "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Quiet", "[", 
         RowBox[{"RemoveScheduledTask", "[", 
          RowBox[{"task", "[", 
           RowBox[{"1", ",", "nb"}], "]"}], "]"}], "]"}], ";", 
        "\[IndentingNewLine]", 
