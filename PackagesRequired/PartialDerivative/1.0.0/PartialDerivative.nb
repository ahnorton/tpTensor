Cell[BoxData[
 RowBox[{
  RowBox[{"pDData", "[", "X_", "]"}], ":=", 
  RowBox[{"Select", "[", 
   RowBox[{
    RowBox[{"pDData", "[", "]"}], ",", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"First", "[", "#", "]"}], "===", "X"}], ")"}], "&"}]}], 
   "]"}]}]], "Input",
 InitializationCell->True],

Cell["\<\
The data for X in a readable form (look like assignments).   pDValues[ ]  \
returns all data,\
\>", "Text"],

Cell[BoxData[
 RowBox[{
  RowBox[{"pDValues", "[", "X___", "]"}], ":=", 
  RowBox[{"Map", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"Apply", "[", 
        RowBox[{
         RowBox[{"HoldForm", "[", "pD", "]"}], ",", 
         RowBox[{"Most", "[", "#", "]"}]}], "]"}], "\[Equal]", 
       RowBox[{"Last", "[", "#", "]"}]}], ")"}], "&"}], ",", 
    RowBox[{"pDData", "[", "X", "]"}]}], "]"}]}]], "Input",
 InitializationCell->True],

Cell["\<\
To make an assignment of the form    pD[ X, a__ ] := val    use this instead \
if you also want the differential consequences to be inferred.  \
\>", "Text"],

Cell["\<\
It would nice to do  TagSet  but tags have to be symbols, which means \
something like  pD[ x[0] , \[Tau]  ]   could not be set a value.\
\>", "Text"],

Cell["\<\
ToString  is beter to use than  ToBoxes   because if   X   has formatting we \
want the InputForm  of  X,\
\>", "Text"],

Cell[BoxData[
 RowBox[{
  RowBox[{"SetpD", "[", 
   RowBox[{"X_", ",", "a__", ",", "val_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"b", ",", "bs", ",", "Xs", ",", "vs", ",", "str"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"b", "=", 
      RowBox[{"SortBy", "[", 
       RowBox[{
        RowBox[{"{", "a", "}"}], ",", "pDIndex"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"bs", "=", 
      RowBox[{"ToString", "[", 
       RowBox[{"b", ",", "InputForm"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Xs", "=", 
      RowBox[{"ToString", "[", 
       RowBox[{"X", ",", "InputForm"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"vs", "=", 
      RowBox[{"ToString", "[", 
       RowBox[{"val", ",", "InputForm"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"str", " ", "=", " ", 
      RowBox[{
      "\"\<pD[\>\"", "<>", "Xs", "<>", "\"\<,c$__] /; ContainsAll[{c$},\>\"", 
       "<>", "bs", "<>", "\"\<]:= pD[\>\"", "<>", "vs", "<>", 
       "\"\<, Apply[Sequence,DeleteElements[{c$},\>\"", "<>", "bs", "<>", 
       "\"\<]]]\>\""}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"ToExpression", "[", "str", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"pDData", "[", "]"}], "=", 
      RowBox[{"Append", "[", 
       RowBox[{
        RowBox[{"DeleteCases", "[", 
         RowBox[{
          RowBox[{"pDData", "[", "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"X", ",", 
            RowBox[{"Apply", "[", 
             RowBox[{"Sequence", ",", " ", "b"}], "]"}], ",", "_"}], "}"}]}], 
         "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"X", ",", 
          RowBox[{"Apply", "[", 
           RowBox[{"Sequence", ",", " ", "b"}], "]"}], ",", "val"}], "}"}]}], 
       "]"}]}], ";", "\[IndentingNewLine]", "str"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 InitializationCell->True],

