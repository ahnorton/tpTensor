Cell[BoxData[
 RowBox[{
  RowBox[{"TeXit", "[", 
   RowBox[{"x_", ",", "label_String"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "s1", ",", "s2", ",", "s3", ",", "s4", ",", "s5", ",", "f", ",", "r", 
      ",", "n"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"!", 
        RowBox[{
         RowBox[{"Head", "[", "x", "]"}], "===", " ", "String"}]}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"s1", "=", 
         RowBox[{"ToString", "[", 
          RowBox[{"TeXForm", "[", "x", "]"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"label", "===", "\"\<\>\""}], ",", "\[IndentingNewLine]", 
          RowBox[{"s2", "=", 
           RowBox[{
           "\"\<$$\\n\>\"", "<>", "\[IndentingNewLine]", "s1", "<>", 
            "\[IndentingNewLine]", "\"\<\\n$$\\n\>\""}]}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{"s2", "=", 
           RowBox[{
           "\"\<\\\\begin{equation}\\n\>\"", "<>", "\[IndentingNewLine]", 
            "s1", "<>", "\[IndentingNewLine]", "\"\<\\n\\\\label{\>\"", "<>", 
            
            RowBox[{"ToString", "[", "label", "]"}], "<>", "\"\<}\>\"", "<>", 
            "\[IndentingNewLine]", "\"\<\\n\\\\end{equation}\\n\>\""}]}]}], 
         "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"s3", "=", 
         RowBox[{"StringReplace", "[", 
          RowBox[{"s2", ",", 
           RowBox[{"\"\<+\>\"", "\[Rule]", "\"\<\\n + \>\""}]}], "]"}]}], ";",
         "\[IndentingNewLine]", 
        RowBox[{"s4", "=", 
         RowBox[{"StringReplace", "[", 
          RowBox[{"s3", ",", 
           RowBox[{"\"\<-\>\"", "\[Rule]", "\"\<\\n - \>\""}]}], "]"}]}], ";",
         "\[IndentingNewLine]", 
        RowBox[{"s5", "=", 
         RowBox[{"StringReplace", "[", 
          RowBox[{"s4", ",", "$TeXReplacements"}], "]"}]}]}], 
       "\[IndentingNewLine]", ",", "\[IndentingNewLine]", 
