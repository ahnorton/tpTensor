   3.681700309613242*^9}, {3.681700392012291*^9, 3.681700454100113*^9}, 
   3.681700643497121*^9, {3.681701203330309*^9, 3.6817012644820232`*^9}, {
   3.681701761163684*^9, 3.68170183396332*^9}, {3.6817033914643*^9, 
