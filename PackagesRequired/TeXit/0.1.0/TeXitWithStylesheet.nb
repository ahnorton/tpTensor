     "\[IndentingNewLine]", 
     RowBox[{"WriteString", "[", 
      RowBox[{"$TeXitFragment", ",", "s5"}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Close", "[", "$TeXitFragment", "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Run", "[", 
      RowBox[{
