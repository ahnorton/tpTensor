Cell[CellGroupData[{

Cell["\<\
Slots[ ]    ---   for MTW machine notation, form notation, and for taking \
components\
\>", "Subsection",
 CellChangeTimes->{{3.621678433593491*^9, 3.621678477417202*^9}, {
  3.621678613498807*^9, 3.621678634250805*^9}}],

Cell[TextData[{
 "Wrapping   Slots[ ]   around a tensor   X   of degree  r   creates a \
tensor-valued function that accepts a sequence of up to  r  vectors and \
1-forms for its arguments.\nIn particular,  components  of  X  w.r.t.  a \
basis/frame  {{\[Omega]0, \[Omega]1, \[Omega]2, \[Omega]3},  {e0, e1, e2, \
e3}}   are obtained as, for example,\n\n                                      \
                 ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    SubscriptBox[
     SuperscriptBox["X", 
      RowBox[{"\[VeryThinSpace]", "i"}]], 
     RowBox[{"j", "\[VeryThinSpace]", "k"}]], 
    RowBox[{"l", "\[VeryThinSpace]", "m"}]], TraditionalForm]]],
 "  =   ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"Slots", "\[VeryThinSpace]", "[", " ", "X", " ", "]"}], 
     "\[VeryThinSpace]", "[", " ", 
     RowBox[{
      SuperscriptBox["\[Omega]", 
       RowBox[{"\[VeryThinSpace]", "i"}]], ",", " ", 
      SubscriptBox["e", "j"], " ", ",", " ", 
      SubscriptBox["e", 
       RowBox[{"\[VeryThinSpace]", "k"}]], " ", ",", " ", 
      SuperscriptBox["\[Omega]", 
       RowBox[{"\[VeryThinSpace]", "l"}]], " ", ",", " ", 
      SuperscriptBox["\[Omega]", 
       RowBox[{"\[VeryThinSpace]", "m"}]]}], " ", "]"}], " "}], 
   TraditionalForm]]],
 "\n                                                       \n                 \
                                                     =    ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[LeftAngleBracket]", " ", 
    RowBox[{
     RowBox[{"\[LeftAngleBracket]", " ", 
      RowBox[{
       RowBox[{"\[LeftAngleBracket]", " ", 
        RowBox[{
         RowBox[{"\[LeftAngleBracket]", " ", 
          RowBox[{
           RowBox[{"\[LeftAngleBracket]", " ", 
            RowBox[{"X", ",", 
             SuperscriptBox["\[Omega]", 
              RowBox[{"\[VeryThinSpace]", "m"}]]}], " ", 
            "\[RightAngleBracket]"}], " ", ",", " ", 
           SuperscriptBox["\[Omega]", 
            RowBox[{"\[VeryThinSpace]", "l"}]]}], " ", 
          "\[RightAngleBracket]"}], " ", ",", " ", 
         SubscriptBox["e", 
          RowBox[{"\[VeryThinSpace]", "k"}]]}], " ", "\[RightAngleBracket]"}],
        " ", ",", 
       SubscriptBox["e", 
        RowBox[{"\[VeryThinSpace]", "j"}]]}], " ", "\[RightAngleBracket]"}], 
     " ", ",", 
     SuperscriptBox["\[Omega]", 
      RowBox[{"\[VeryThinSpace]", "i"}]]}], " ", "\[RightAngleBracket]"}], 
   TraditionalForm]]],
 "  .\n                                                                      \
\nLast slots are filled first.  Eg,  for the above X, \n\n                    \
                       ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"Slots", "\[VeryThinSpace]", "[", " ", "X", " ", "]"}], 
     "\[VeryThinSpace]", "[", "  ", 
     RowBox[{
      SubscriptBox["e", 
       RowBox[{"\[VeryThinSpace]", "k"}]], " ", ",", " ", 
      SuperscriptBox["\[Omega]", 
       RowBox[{"\[VeryThinSpace]", "l"}]], " ", ",", " ", 
      SuperscriptBox["\[Omega]", 
       RowBox[{"\[VeryThinSpace]", "m"}]]}], " ", "]"}], " "}], 
   TraditionalForm]]],
 "    =    ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[LeftAngleBracket]", " ", 
    RowBox[{
     RowBox[{"\[LeftAngleBracket]", " ", 
      RowBox[{
       RowBox[{"\[LeftAngleBracket]", " ", 
        RowBox[{"X", ",", " ", 
         SubscriptBox["e", 
          RowBox[{"\[VeryThinSpace]", "k"}]]}], " ", "\[RightAngleBracket]"}],
        " ", ",", 
       SubscriptBox["e", 
        RowBox[{"\[VeryThinSpace]", "j"}]]}], " ", "\[RightAngleBracket]"}], 
     " ", ",", 
     SuperscriptBox["\[Omega]", 
      RowBox[{"\[VeryThinSpace]", "i"}]]}], " ", "\[RightAngleBracket]"}], 
   TraditionalForm]]],
 " \n\n                                                                       \
                      =    ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    SubscriptBox[
     SuperscriptBox["X", 
      RowBox[{"\[VeryThinSpace]", "i"}]], 
     RowBox[{"j", "\[VeryThinSpace]", "k"}]], 
    RowBox[{"l", "\[VeryThinSpace]", "m"}]], TraditionalForm]]],
 "  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["e", 
     RowBox[{"\[VeryThinSpace]", "i"}]], " ", "\[CircleTimes]", " ", 
    SuperscriptBox["\[Omega]", 
     RowBox[{"\[VeryThinSpace]", "j"}]]}], TraditionalForm]]],
 " .  \n"
}], "Text",
 CellChangeTimes->{{3.621678498249445*^9, 3.6216784993387547`*^9}, {
